<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github remote -->
  <remote name="github"
          fetch="https://github.com/" />

  <!-- AOSP-VICTARA remote -->
  <remote name="aosp-victara"
          revision="oreo"
          fetch="https://github.com/aosp-victara/" />

  <!-- general -->
  <project path="build/make" name="build_make" remote="aosp-victara" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" remote="aosp-victara">
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="build/kati" name="build_kati" remote="aosp-victara" />
  <project path="manifest" name="manifest" remote="aosp-victara" />
  <project path="bionic" name="bionic" remote="aosp-victara" />
  <project path="bootable/recovery" name="bootable_recovery" remote="aosp-victara" />

  <!-- system -->
  <project path="system/vold" name="system_vold" remote="aosp-victara" />
  <project path="system/core" name="system_core" remote="aosp-victara" />
  <project path="system/sepolicy" name="system_sepolicy" remote="aosp-victara" />
  <project path="system/media" name="system_media" remote="aosp-victara" />

  <!-- sepolicy -->
  <project path="device/qcom/sepolicy" name="device_qcom_sepolicy" remote="aosp-victara" />

  <!-- frameworks -->
  <project path="frameworks/native" name="frameworks_native" remote="aosp-victara" />
  <project path="frameworks/av" name="frameworks_av" remote="aosp-victara" />

  <!-- hardware -->
  <project path="hardware/qcom/bt" name="hardware_qcom_bt" remote="aosp-victara" />
  <project path="hardware/interfaces" name="hardware_interfaces" remote="aosp-victara" />
  <project path="hardware/ril" name="hardware_ril" remote="aosp-victara" />

</manifest>
